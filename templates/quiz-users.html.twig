<h6>Users</h6>
<table class="table table-striped">
	<thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Password</th>
        <th>status</th>        
      </tr>
    </thead>
    <tbody>
		{% for user in content.users %}
	      <tr>
	        <td>{{user.name}}</td>
	        <td>{{user.email}}</td>
	        <td>{{user.password}}</td>
	        <td>{{user.status}}</td>
	      </tr>		    
		{% endfor %}
    </tbody>
</table>

<h6>Add User</h6>

{{content.form}}